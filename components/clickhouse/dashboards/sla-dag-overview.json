      "pluginVersion": "12.3.1",
      "targets": [
        {
          "format": "time_series",
          "query": "WITH final AS (\n  SELECT\n    run_id,\n    toStartOfInterval(requested_at, INTERVAL 5 minute) AS t,\n    argMax(status, event_time)     AS final_status,\n    argMax(latency_ms, event_time) AS latency_ms\n  FROM reports.report_runs_v\n  WHERE requested_at BETWEEN toDateTime(intDiv(${__from}, 1000), 'UTC')\n                        AND toDateTime(intDiv(${__to}, 1000), 'UTC')\n  GROUP BY run_id, t\n)\nSELECT\n  t AS time,\n  100.0 * countIf(final_status = 'SUCCESS' AND latency_ms <= 10000)\n       / nullIf(countIf(final_status IN ('SUCCESS','FAILED')), 0) AS sla_le_10s_pct,\n  100.0 * countIf(final_status = 'SUCCESS' AND latency_ms <= 30000)\n       / nullIf(countIf(final_status IN ('SUCCESS','FAILED')), 0) AS sla_le_30s_pct\nFROM final\nGROUP BY time\nORDER BY time\n",
          "rawQuery": true,
          "refId": "A"
        }
      ],
      "title": "SLA % Met Over Time (10s & 30s)",
      "type": "timeseries"
    },
